{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Server",
  "extends": "./server-base.json",
  "compilerOptions": {
    "lib": ["ES2021"],
    "module": "commonjs",
    "target": "ES2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
